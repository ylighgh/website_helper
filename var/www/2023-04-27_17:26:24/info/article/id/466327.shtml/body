	K.scroll({                
		id:"picScroll",         
		effect:"scroll",           
		scWr:".picList",               
		scrElem:".picItem",       
		scrNum:1,       
		auto:1,            
		vertical:"left",    
		loop:true,           
		interval:5000,           
		bns: ['.picPrev', '.picNext'],
		prevMouseClass:"onPrev",
		nextMouseClass:"onNext"
	})   
})  
Qfast(false,'widgets', function () {  
	K.tabs({  
		id: 'navtl',    
		tabId: "navchange_tab", 
		tabCn:'.lk', 
		conId: "navchange_con",                
		conCn: '.split',            
